programa {
  funcao inicio() {
    
    real n

    escreva("Digite o valor do número: ")
    leia(n)

    se(n > 10){
      escreva("O número é maior que 10!")
    }
    se(n == 10){
      escreva("O valor é igual a 10!")
    }senao se(n < 10){
      escreva("O número é menor que 10!")
    }
  }
}
